-- Module Scripts:

local fake_module_scripts = {}

do -- ScreenGui.ModuleScript
	local script = Instance.new('ModuleScript', ScreenGui)
	local function module_script()
		local module = {}
		
		return module
		
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function SIYHLXF_fake_script() -- AUTOOPENSCRIT.LocalScript 
	local script = Instance.new('LocalScript', AUTOOPENSCRIT)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	loadstring(game:HttpGet("https://raw.githubusercontent.com/naypramx/Simulator/Script/AnimeYedhee", true))()
	
end
coroutine.wrap(SIYHLXF_fake_script)()
